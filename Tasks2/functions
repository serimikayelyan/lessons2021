function task155
{
    sum=0

    for i in {10..99}
    do
        if [[ $i%3 -eq 0 ]]
	then
            let sum=$sum+$i
	fi
    done
    echo $sum
}

function task156
{
    sum=1

    for i in {10..99}
    do 
	if [[ $i%3 -eq 0 && $i%5 -eq 0 ]]
	then
	    let sum=$sum*$i
	fi
    done
    echo $sum
}

function task157
{
    sum=0

    for i in {100..999}
    do
        if [[ $i%5 -ne 0 ]]
	then
	    let	sum=$sum+$i
	fi
    done
    echo $sum
}

function task158
{
    sum=1

    for i in {100..999}
    do
	if [[ $i%2 -ne 0 && $i%3 -ne 0 ]]
	then
	    let sum=$sum*$i
	fi
    done
    echo $sum
}

function task159
{
    sum=1

    for i in {100..999}
    do
	if [[ $i%3 -eq 1 && $i%4 -eq 2 ]]
	then
	    let sum=$sum*$i
	fi
    done
    echo $sum
}

function task160 
{
    for i in {100..999}
    do
	let sum=($i*16)

	#squareRoot is variable that calculates root of num then turns to string to check numbers after point
	squareRoot="$(bc <<< "scale=2; sqrt($sum)")"

	for (( x = 0; x <= ${#squareRoot}; x++ ))
	do
		if [[ ${squareRoot:$x:1} = "." ]]
		then
			let endPoint=(${#squareRoot}-$x)
			let startPoint=($x+1)

			newstr=${squareRoot:$startPoint:$endPoint}

			if [[ $newstr == "00" ]]
			then
				echo $i
				exit 0
			fi
		fi
	done
    done
}

function task161
{
    for i in {1000..9999}
    do
	let sum=($i*26)

	sqrRoot="$(bc <<< "scale=2; sqrt($sum)")"
	
	for (( x = 0; x <= ${#sqrRoot}; x++ ))
	do
		if [[ ${sqrRoot:$x:1} == "." ]]
		then
			let startPoint=($x+1)
			let endPoint=(${#sqrRoot}-$x)

			newstr=${sqrRoot:$startPoint:$endPoint}

			if [[ $newstr == "00" ]]
			then
				echo $i
				exit 0
			fi
		fi

	done

    done

}

function task162
{
    for i in {9999..1000}
