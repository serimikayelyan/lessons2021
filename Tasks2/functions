		if [[ ${sqrRoot:$x:1} == "." ]]
		then
			let startPoint=($x+1)
			let endPoint=(${#sqrRoot}-$x)

			newstr=${sqrRoot:$startPoint:$endPoint}

			if [[ $newstr == "00" ]]
			then
				echo $i
				exit 0
			fi
		fi
	done
    done
}

function task163
{
    for i in {9999..1000}
    do
	let sum=($i*18)

	sqrRoot="$(bc <<< "scale=2; sqrt($sum)")"

	for (( x = 0; x <= ${#sqrRoot}; x++ ))
	do
		if [[ ${sqrRoot:$x:1} == "." ]]
		then
			let startPoint=($x+1)
			let endPoint=(${#sqrRoot}-$x)
			
			newstr=${sqrRoot:$startPoint:$endPoint}

			if [[ $newstr == "00" ]]
			then
				echo $i
				exit 0
			fi
		fi
	done
    done
}

function task164
{
    for i in {100..999}
    do
	sum=$(bc <<< "scale=2; sqrt($i)")
	if [[ $sum > $1 ]]
	then
		echo $i
		exit 0
	fi
    done
}


