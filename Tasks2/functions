#!/bin/bash

function task151
{
    sum=0;
    #$1 is given argument

    for (( i = 1; i <= $1/2; i++ ))
    do
	    if [[ $1%$i -eq 0 ]]
	    then
		let sum=$sum+$i
	    fi
    done
    echo $sum
}

function task152
{
    sum=1;
    #$1 is given argument

    for (( i = 1; i <= $1/2; i++ ))
    do
	    if [[ $1%$i -eq 0 ]]
	    then
		let sum=$sum*$i
	    fi
    done
    echo $sum
}

