#!/bin/bash

s=0
c=0
arr=( -2 33 -4 8 -4 )
  for i in ${arr[@]}
    do
      if [[ $i -lt 0 ]]
        then
          i=$(($i*$i))
          s=$(($s+$i))
          c=$(($c+1))
      fi
    done
d=$(($s/$c))
echo $d

