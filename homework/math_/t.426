#!/bin/bash


declare -A arr
s=0
v=0
n=0
for ((i=0; i<$1; i++))
do
    for ((j=0; j<$1; j++))
    do
    arr[$i,$j]=$(($i+$j))
    v=${arr[$i,$j]}
      if [[ $v%2 -eq 0 && $(($i+$j))<$(($1-1)) ]]
      then
      s=$(($s+$v))
      fi
       echo -n ${arr[$i,$j]} 
    done
       echo ""
done
       echo $s
