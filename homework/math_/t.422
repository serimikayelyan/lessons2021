#!/bin/bash

declare -A arr
sum=0
num=0 
var=0

for ((i=0; i<$1; i++))
do
    for ((j=0; j<$1; j++))
    do
      	    arr[$i,$j]=$(($i+$j+4))
     var=${arr[$i,$j]}
       if [[ $var%5 -eq 0 && $j<$i ]]
         then 
           sum=$(($sum+$var))
           num=$(($num+1))
       fi
          
           echo -n ${arr[$i,$j]}

    done
           echo ""
done
           s=$(($sum/$num))
           echo $s

